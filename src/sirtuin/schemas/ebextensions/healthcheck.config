option_settings:

  aws:elasticbeanstalk:environment:process:default:
    Protocol: "$_ProcessProtocol_$"
    Port: "$_ProcessPort_$"
    StickinessEnabled: "$_StickinessEnabled_$"
    StickinessLBCookieDuration: "$_StickinessLBCookieDuration_$"
    HealthCheckPath: "$_HealthCheckPath_$"
    HealthyThresholdCount: "3"
    UnhealthyThresholdCount: "3"
    HealthCheckInterval: "$_HealthCheckInterval_$"
    HealthCheckTimeout: "$_HealthCheckTimeout_$"
    DeregistrationDelay: "20"
